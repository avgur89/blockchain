+b.SECTION.logo_blue
	.container.container_large
		+b.title_white-dark Инвесторы
			span и партнеры
		.container
			- var logoList = ['static/img/logo-9.svg', 'static/img/logo-10.svg', 'static/img/logo-11.svg', 'static/img/logo-12.svg', 'static/img/logo-13.svg', 'static/img/logo-14.svg', 'static/img/logo-15.svg', 'static/img/logo-16.svg']
			each item, index in logoList
				if index > 3
					+e.item(data-text-anim data-delay= 2 * (index - 4))
						img(src=item alt='image')
				else
					+e.item(data-text-anim data-delay= 2*index)
						img(srcset=item alt='image')